type Account @goModel(model: "$VENDOR/accounts/internal/models.Account") {
  id: ID!
  bankName: String!
  accountNumber: String!
  holder: String!
  created: Time!
}

input CreateAccountInput @goModel(model: "$VENDOR/accounts/internal/cases/account.CreateRequest") {
  bankName: String!
  accountNumber: String!
  holder: String!
}

input UpdateAccountInput @goModel(model: "$VENDOR/accounts/internal/cases/account.UpdateRequest") {
  bankName: String
  accountNumber: String
  holder: String
}
